{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "prettier",
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"prettier",
		"prefer-import"
	],
	"rules": {
		"prettier/prettier": "warn",
		"prefer-import/prefer-import-over-require": [
			"warn"
		]
	}
}